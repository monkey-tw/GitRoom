pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }

        stage("clean") {
            steps {
                xcodebuild clean -project GitRoom -scheme GitRoom -configuration ${GITROOM_CONFIG}
                echo "xcodebuild clean ${GITROOM_CONFIG} successfully"
            }
            
        }
    }
}
